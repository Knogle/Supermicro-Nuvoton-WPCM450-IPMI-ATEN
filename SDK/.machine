
#MYMCU setting 
MYMCU-$(CONFIG_CPU_ASPEED_2050) = AST2050
MYMCU-$(CONFIG_CPU_WINBOND_HERMON) = HERMON
MYMCU-$(CONFIG_CPU_NUVOTON_YARKON) = YARKON
MYMCU = $(MYMCU-y)

#BOARD setting
BOARD-$(CONFIG_PLATFORM_AST2050EVB) = AST2050_EVB
BOARD-$(CONFIG_PLATFORM_HERMONEVB) = HERMON_EVB
BOARD-$(CONFIG_PLATFORM_YARKONEVB) = YARKON_EVB
BOARD-$(CONFIG_PLATFORM_TWIN_CASTOR) = TWIN_CASTOR
BOARD-$(CONFIG_PLATFORM_SUPERMICRO_X7SB3) = SuperMicro_X7SB3
BOARD-$(CONFIG_PLATFORM_IP1500) = IP1500
BOARD-$(CONFIG_PLATFORM_IP1600) = IP1600
BOARD-$(CONFIG_PLATFORM_WISTRON_P92TB1) = Wistron_P92TB1
BOARD-$(CONFIG_PLATFORM_ADLINK_CMM) = ADLINK_CMM
BOARD-$(CONFIG_PLATFORM_HW_STG2SMCA) = HW_STG2SMCA
BOARD-$(CONFIG_PLATFORM_NEXCOM_NSB7110) = NEXCOM_NSB7110
ifneq ($(BOARD),)
BOARD_DEF = 1
endif
BOARD = $(BOARD-y)
ifeq ($(CONFIG_PLATFORM_SUPERMICRO_SUBVERSION_X8DTT),y)
SUBBOARD = OEM_X8DTT
endif

#WEBSTYLE setting
WEBSTYLE-$(CONFIG_SAMPLE_ATEN) = ATEN_legacy
WEBSTYLE-$(CONFIG_SAMPLE_ATEN_NEW) = ATEN
WEBSTYLE-$(CONFIG_WISTRON_P92TB1) = WISTRON_P92TB1
WEBSTYLE-$(CONFIG_SAMPLE_SUPER_MICRO_X8DTL) = SUPER_MICRO_X8DTL
WEBSTYLE-$(CONFIG_SAMPLE_ALTUSEN) = ALTUSEN
WEBSTYLE-$(CONFIG_SAMPLE_DEMOALLIN1) = DEMO_ALLIN1
WEBSTYLE-$(CONFIG_OEM_WEB) = OEM_WEB
WEBSTYLE-$(CONFIG_ADLINK_CMM) = ADLINK_CMM
WEBSTYLE-$(CONFIG_SUPERMICRO_2010) = SUPERMICRO
WEBSTYLE-$(CONFIG_NEXCOM) = NEXCOM
ifeq ($(WEBSTYLE-y),OEM_WEB)
WEBSTYLE-y = ATEN_legacy
WEBSTYLESUB = OEM_WEB
endif
WEBSTYLE = $(WEBSTYLE-y)

